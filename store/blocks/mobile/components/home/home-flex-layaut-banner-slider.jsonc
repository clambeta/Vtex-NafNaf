{
  "flex-layout.row#mobile__home--mid-banner-slider-content": {
    "title": "Banner de imagen y slider",
    "children": ["flex-layout.col#mobile__mid--banner-slider-content"],
    "props": {
      "blockClass": "mobile-mid--banner-slider-content",
      "preserveLayoutOnMobile": true,
      "colGap": 2,
      "colSizing": "auto",
      "paddingTop": 4,
      "paddingBottom": 8,
      "fullWidth": true
    }
    },
    "flex-layout.col#mobile__mid--banner-slider-content": {
      "title": "Contenedor del banner-slider",
      "children": [
        "flex-layout.col#mobile__mid--banner-slider-content-one",
        "flex-layout.row#mobile__mid--banner-slider-content-two"
      ],
      "props": {
        "width": "100%",
        "rowGap": 3,
        "blockClass": "mobile-bottom--banner-slider-content-col"
      }
    },
    "flex-layout.col#mobile__mid--banner-slider-content-one":{
      "title": "Imagen del banner-slider",
      "children": ["image#mobile__home__mid--banner-slider"],
      "props": {
        "width": "25%",
        "rowGap": 2,
        "blockClass": "mobile-mid--banner-slider-content-image"
      }
    },
    "image#mobile__home__mid--banner-slider": {
      "title": "Imagen del banner-slider mobile",
      "props": {
        "blockClass": "mobile-banner-slider-img",
        "src": "assets/img/neutral-vibes.png",
        "link": {
          "url": "https://www.nafnaf.com.co/new-arrivals"
        }
      }
    },
    "flex-layout.row#mobile__mid--banner-slider-content-two": {
      "title": "Contenedor del slider de la secci√≥n banner-slider",
      "children": ["slider-layout#mobile__mid--banner-slider-content-two"],
      "props": {
        "blockClass": "mobile-banner-slider-col",
        "preload": true,
        "width": "75%",
        "colGap": 2
      }
      }
    ,
    "slider-layout#mobile__mid--banner-slider-content-two": {
      "title": "Card con imagen y texto del slider",
      "children": [
        "flex-layout.col#mobile__mid--banner-slider-one",
        "flex-layout.col#mobile__mid--banner-slider-two",
        "flex-layout.col#mobile__mid--banner-slider-three",
        "flex-layout.col#mobile__mid--banner-slider-four",
        "flex-layout.col#mobile__mid--banner-slider-five"
      ],
      "props": {
        "blockClass": "mobile-mid--banner-slider-content-card",
        "itemsPerPage": {
          "desktop": 3,
          "tablet": 1,
          "mobile": 1
        },
        "infinite": true,
        "showPaginationDots": "never",
        "centerMode": "center",
        "arrowSize": 10,
        "centerModeSlidesGap": 5
      }
    },
    "flex-layout.col#mobile__mid--banner-slider-one": {
      "title": "Columna slider one",
      "children": [
        "image#mobile__home__mid--banner-slider-one",
        "rich-text#mobile__home__mid--banner-slider-card-one"
      ],
      "props": {
        "preventVerticalStretch": true,
        "horizontalAlign": "center",
        "rowGap": 2,
        "blockClass": "mobile-mid--banner-slider-card"
      }
    },
